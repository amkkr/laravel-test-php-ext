parameters:
    level: max
    ignoreErrors:
        - '#Call to an undefined static method Illuminate\\Support\\Facades\\Redis::[a-zA-Z0-9\\_]+\(\)#'
    scanFiles:
        - _ide_helper.php
    paths:
        - app
    excludePaths:
        - app/vendor
        - app/Console
        - app/Events
        - app/Exceptions
        - app/Jobs
        - app/Listeners
        - app/Providers
